{
    Dictionary<string, HashSet<string>> ReceiptToCustomerMap; // Using dictionary to track Reciept id to Customer mapping
    Dictionary<string, HashSet<string>> StaffToNameMap; // Using dictionary to track Staff ID to Staff Name mapping

    public override void PreExecute()
    {
        base.PreExecute();
        ReceiptToCustomerMap = new Dictionary<string, HashSet<string>>();
